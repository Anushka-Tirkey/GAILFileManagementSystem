@model IEnumerable<GAILFileManagementSystem.Models.Files>

@{
    ViewData["Title"] = "GenerateConsolidatedReport";
}

<html>
    <head>
    <style>

        .material-symbols-outlined {
            font-variation-settings: 'FILL' 0, 'wght' 400, 'GRAD' 0, 'opsz' 24;
            font-size: 120px;
        }

        body {
            margin: 0px;
            font-family: Arial, Helvetica, sans-serif;
            background-image: url('/images/GAILbg.jpg');
            color: black;
            background-size: cover;
            background-repeat: no-repeat;
            background-position: center;
        }

        nav {
            background-color: rgb(64, 143, 247);
            height: 58px;
            padding-top: 0.1px;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
        }

        * {
            box-sizing: border-box;
        }

        .container1 {
            width: 80% /* 2500px; */
            height: 100%;
            background-color: rgb(255, 230, 0);
            margin: 1px auto;
            padding-top: 0;
            border-radius: 10px;
            padding-bottom: 20px;
            border-radius: 5px;
            border: 1px solid #ccc;
        }

        fieldset {
            background-color: whitesmoke;
            margin: 30px;
            border-radius: 8px;
            border: 1px solid #ccc;
        }

        legend {
            padding: 8px;
            background-color: #BDE3FF;
            border-radius: 8px;
        }

        h2 {
            text-align: center;
        }

        .vendor-details {
            margin-top: 20px;
        }

        .table-form {
            width: 100%;
            border-collapse: collapse;
            margin-top: 10px;
        }

            .table-form th, .table-form td {
                padding: 10px;
                text-align: left;
                border-bottom: 1px solid #ddd;
            }

            .table-form input[type="text"], .table-form input[type="date"], .table-form select {
                width: 100%;
                padding: 10px;
                border: 1px solid #ccc;
                border-radius: 4px;
                box-sizing: border-box;
            }

            .table-form select {
                -webkit-appearance: none;
                -moz-appearance: none;
                appearance: none;
                background: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='14' height='14' viewBox='0 0 14 14'><path d='M7 10l-5-5 5-5 1 1 4 4-4 4z'/></svg>") no-repeat right 10px center;
            }

            .table-form input[type="date"]::-webkit-calendar-picker-indicator {
                background: none;
            }

        .button-container {
            margin-top: 20px;
            display: flex;
            justify-content: center;
            gap: 10px;
        }

        .profile-image {
            padding-top: 20px;
            padding-left: 50px;
            width: 50px;
            height: 50px;
            border-radius: 50%;
            margin-right: 10px;
        }

        .light {
            background-color: #BDE3FF;
            height: 20px;
        }

        .input-container {
            position: relative;
            width: 300px; /* Adjust the width as needed */
        }

            .input-container input[type="date"] {
                width: 100%;
                padding-right: 30px; /* Space for the icon */
                box-sizing: border-box;
            }

            .input-container .icon {
                position: absolute;
                right: 30px;
                top: 50%;
                transform: translateY(-50%);
                pointer-events: none; /* Makes sure the icon does not block input clicks */
            }

        .user-info {
            display: flex;
        }

        .icon-file-input-container {
            display: flex;
            align-items: center;
        }

            .icon-file-input-container .icon {
                margin-right: 8px;
            }

            .icon-file-input-container input[type="file"] {
                display: inline-block;
            }

        #enter {
            background-color: rgb(64, 143, 247);
            width: 300px;
            height: 300px;
            border-radius: 10px;
            cursor: pointer;
            text-align: center;
            font-size: 25px;
        }

        #enter:hover {
                /* transform: scale(1.1); */
                filter: brightness(1.30);
                box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
        }
    </style>
    </head>
<body>

